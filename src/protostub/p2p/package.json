{
  "name": "P2PStubs",
  "version": "0.1.0",
  "description": "Implementation of the P2P handler and requester stub",
  "main": "",
  "scripts": {
    "prepare": "mkdir -p dist",
    "build_handler": "browserify src/P2PHandlerStub.js --standalone activate -o dist/P2PHandlerProtoStub.js",
    "build_requester": "browserify src/P2PRequesterStub.js --standalone activate -o dist/P2PRequesterProtoStub.js",
    "build": "npm run prepare && npm run build_handler && npm run build_requester",
    "start": "npm run build",
    "init-setup": "npm install browserify babelify && npm install"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/reTHINK-project/dev-protostubs.git"
  },
  "keywords": [
    "reThink",
    "webRTC",
    "p2p"
  ],
  "author": "reThink Project",
  "contributors": [
    "Steffen Dr√ºsedow <steffen.druesedow@telekom.de>"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reTHINK-project/dev-msg-node-nomatrix/issues"
  },
  "homepage": "https://rethink-project.eu/",
  "devDependencies": {
    "babel-cli": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.1.8",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "jsdoc": "git+https://github.com/jsdoc3/jsdoc.git",
    "promise": "^7.0.4",
    "url": "^0.11.0",
    "yargs": "^5.0.0"
  },
  "dependencies": {
    "service-framework": "github:rethink-project/dev-service-framework#dev-p2p-interworking-merge",
    "webrtc-adapter": "^3.1.0"
  }
}
