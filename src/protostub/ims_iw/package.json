{
  "name": "ims_iwstub",
  "version": "0.2.0",
  "description": "A protostub to interact with an IMS lab environment.",
  "scripts": {
    "prepare": "mkdir -p dist",
    "build_handler": "browserify src/IMSIWProtoStub.js --standalone activate -o dist/IMSIWProtoStub.js",
    "build_requester": "browserify src/IMSIWProxyStub.js --standalone activate -o dist/IMSIWProxyStub.js",
    "build": "npm run prepare && npm run build_handler && npm run build_requester",
    "start": "npm run build",
    "init-setup": "npm install browserify babelify && npm install"
  },
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reTHINK-project/dev-protostubs.git"
  },
  "author": "David VÃ­lchez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/reTHINK-project/dev-protostubs/issues"
  },
  "homepage": "https://github.com/reTHINK-project/dev-protostubs#readme",
  "devDependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.18.0",
    "babelify": "7.3.0",
    "browserify": "13.1.1",
    "chai": "3.5.0",
    "karma": "1.3.0",
    "karma-browserify": "5.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.1",
    "mocha": "3.2.0",
    "proxyquireify": "3.2.1",
    "watchify": "3.8.0",
    "webrtc-adapter": "2.1.0"
  },
  "dependencies": {
    "sdp-transform": "1.7.0",
    "service-framework": "github:rethink-project/dev-service-framework#develop",
    "sip.js": "^0.7.5"
  }
}
